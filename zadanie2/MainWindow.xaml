<Window x:Class="zadanie2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:zadanie2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Input" HorizontalAlignment="Left" Height="23" Margin="22,103,0,0" TextWrapping="Wrap" Text="Input" VerticalAlignment="Top" Width="120" TextChanged="Input_TextChanged"/>
        <Button x:Name="Generuj" Content="Generuj" HorizontalAlignment="Left" Margin="147,107,0,0" VerticalAlignment="Top" Width="75" Click="Generuj_Click"/>
        <Button x:Name="Zapis" Content="Zapis" HorizontalAlignment="Left" Margin="147,131,0,0" VerticalAlignment="Top" Width="75" Click="Zapis_Click"/>
        <Button x:Name="Odczyt" Content="Odczyt" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="147,155,0,0" Click="Odczyt_Click"/>

    </Grid>
</Window>
